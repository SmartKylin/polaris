
.tentacle--icon {
  width 40px
  height 40px
  border-radius 50%
  float right
}

.tentacle--label {
  background #8e7b3a
  overflow hidden
  font-size 12px
  padding 3px
}

.tentacle--wrap {
  background #fff
  padding 10px 30px
  box-shadow 5px 5px 5px #ccc
  a {
    text-decoration none
    color #000
  }
}

.tentaclebar--lastconcat {
  margin-top 30px
}

.btn-group {
  overflow hidden
  button {
    height 30px
    border-radius 5px
    float right
    background #13CE66
    outline none
  }
}
