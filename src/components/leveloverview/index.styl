.tentacleView--tab {
  display: flex;
  background-color: #fff;
}

.tentacleView--tabItem {
  flex: 1;
  text-align: center;
  padding: 10px 5px;
  border-bottom: 2px solid #20a0ff;
  box-sizing: border-box;
}

.tentacleView--tabItem--cont {
  padding-bottom: 5px;
  font-size: 14px;
  color: #353535;
}

.tentacleView--tabItem--name {
  font-size: 12px;
  color: #8492A6;
}

.tentacleView--tabItem.selected {
  background-color: #f5f5f5;
}

.tentacleView--tabItem.selected .tentacleView--tabItem--cont,
.tentacleView--tabItem.selected .tentacleView--tabItem--name {
  color: #fd6322;
}

.mt10 {
  margin-top: 0;
}
.overview--wrap {
  margin 0;
  padding 0
}
