.pick-tentacle {
  overflow: auto;
  position: fixed;
  z-index: 5;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: #fff;
  -webkit-overflow-scrolling: touch;
}

.pick-tentacle--hd {
  height: 50px;
  background-color: #fff;
}

.searchBarWrapper {
  /*position: fixed;
  left: 0;
  right: 0;*/
  display: flex;
  padding: 10px 15px;
  background-color: #20a0ff;
}

.searchBar--btn {
  height: 30px;
  margin-left: 10px;
  line-height: 30px;
  font-size: 14px;
  color: #fff;
  font-size: 14px;
}

.searchBar {
  flex: 1;
  display: flex;
  height: 30px;
  padding: 0 5px;
  align-items: center;
  border-radius: 3px;
  overflow: hidden;
  background-color: #fff;
  background-size: 20px;
  background-position: 2px center;
  background-repeat: no-repeat;
  background-image: url(../../../images/icon-search.svg);
}

.searchBar--ipt {
  width: 100%;
  height: 20px;
  padding-top: 1px;
  padding-left: 20px;
  line-height: 20px;
  font-size: 12px;
  color: #353535;
  background-color: transparent;
}
