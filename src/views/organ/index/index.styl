.organ--body--bg {
  width: 75px;
  height: 75px;
  margin-bottom: 15px;
  margin-top: 150px;
}

.organ--txt {
  line-height: 15px;
  color: #ccc;
  font-size: 15px;
  margin-bottom: 6px;

}

.no--search--result {
  text-align: center;
}

.search--result {
  padding: 0 15px;
  background: #fff;
  border-top: 1px solid #e5e5e5;
  .organ--item {
    height: 45px;
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
    align-items: center;
    >div {
      background: #fff;
      height: 45px;
      font-size: 15px;
      line-height: 45px;
      width: 100%;
      color: #333;
    }
    >img {
      width: 17px;
      height: 17px;
    }

  }
}
